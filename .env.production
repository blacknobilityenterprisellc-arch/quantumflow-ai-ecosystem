DATABASE_URL=postgresql://keystone_user:keystone_password@localhost:5432/keystone_db

# AI Provider API Keys (Production)
OPENAI_API_KEY=sk-proj-unified-keystone-openai-2025
ANTHROPIC_API_KEY=sk-ant-unified-keystone-anthropic-2025
GOOGLE_AI_API_KEY=AIzaUnifiedKeystoneGoogle2025
COHERE_API_KEY=unified-keystone-cohere-2025
