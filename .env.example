# üõ°Ô∏è AETHERIUS-PRIME ENVIRONMENT CONFIGURATION
# Secure environment variables template - DO NOT COMMIT ACTUAL SECRETS

# Database Configuration
DATABASE_URL="file:./dev.db"
DATABASE_URL="postgresql://username:password@localhost:5432/quantumflow"
REDIS_URL="redis://localhost:6379"

# Authentication & Security
JWT_SECRET="your-super-secure-jwt-secret-key-change-this-in-production"
JWT_EXPIRES_IN="7d"
BCRYPT_ROUNDS="12"

# API Keys (External Services)
OPENAI_API_KEY="sk-your-openai-api-key"
ANTHROPIC_API_KEY="sk-ant-your-anthropic-api-key"
GOOGLE_AI_API_KEY="AIza-your-google-ai-api-key"
STRIPE_SECRET_KEY="sk_test_your-stripe-secret-key"
STRIPE_WEBHOOK_SECRET="whsec_your-stripe-webhook-secret"

# Infrastructure Services
AWS_ACCESS_KEY_ID="AKIAyourawsaccesskey"
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your-s3-bucket-name"

# Monitoring & Analytics
SENTRY_DSN="https://your-sentry-dsn"
GOOGLE_ANALYTICS_ID="GA-YOUR-GA-ID"
MIXPANEL_TOKEN="your-mixpanel-token"

# Development & Debug
NODE_ENV="development"
LOG_LEVEL="info"
DEBUG="quantumflow:*"

# Quantum Systems
QUANTUM_COHERENCE_THRESHOLD="0.95"
QUANTUM_PROTECTION_LEVEL="high"
QUANTUM_AUTO_HEALING="true"

# Rate Limiting
RATE_LIMIT_WINDOW_MS="900000"
RATE_LIMIT_MAX_REQUESTS="100"

# Session Management
SESSION_TIMEOUT_MS="3600000"
SESSION_SECRET="your-session-secret-key"

# Email Configuration
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# Cache Configuration
CACHE_TTL_SECONDS="300"
CACHE_MAX_SIZE="1000"

# Feature Flags
ENABLE_QUANTUM_AI="true"
ENABLE_ADVANCED_ANALYTICS="true"
ENABLE_BILLING="true"
ENABLE_MULTI_TENANCY="false"

# Security Headers
CORS_ORIGIN="http://localhost:3000"
ALLOWED_HOSTS="localhost,127.0.0.1"

# Performance
MAX_CONCURRENT_REQUESTS="100"
REQUEST_TIMEOUT_MS="30000"

# Monitoring
HEALTH_CHECK_INTERVAL="30000"
METRICS_COLLECTION_INTERVAL="60000"

# üö® SECURITY REMINDERS:
# 1. Never commit actual secrets to version control
# 2. Use different values for development/staging/production
# 3. Rotate secrets regularly (every 90 days recommended)
# 4. Use strong, randomly generated secrets
# 5. Store secrets in secure vault (AWS Secrets Manager, Azure Key Vault, etc.)
# 6. Use environment-specific .env files (.env.development, .env.production)
# 7. Implement secret scanning in CI/CD pipeline
# 8. Use least privilege principle for API keys
# 9. Enable audit logging for all secret access
# 10. Have secret rotation procedures in place